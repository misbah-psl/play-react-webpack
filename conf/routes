# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index(remPath="")
GET     /app                           controllers.Application.index(remPath="")
GET     /app/*remPath                         controllers.Application.index(remPath)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET        /api/t_benchmarks         controllers.TpcdsController.index
GET        /api/t_benchmark/:id      controllers.TpcdsController.read(id: String)
POST       /api/t_benchmark          controllers.TpcdsController.create_exp
GET        /api/t_benchmarks/graph         controllers.TpcdsController.graph